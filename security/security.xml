<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.module.category" id="module_category_sd_project_planning">
        <field name="name">sd_project_planning</field>
        <field name="description">requests.</field>
    </record>

    <record id="group_sd_project_planning_users" model="res.groups">
        <field name="name">Users</field>
        <field name="category_id" ref="module_category_sd_project_planning"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_sd_project_planning_admins" model="res.groups">
        <field name="name">Administrators</field>
        <field name="category_id" ref="module_category_sd_project_planning"/>
        <field name="implied_ids" eval="[(4, ref('group_sd_project_planning_users'))]"/>
    </record>


    <record id="base.group_system" model="res.groups">
        <field name="implied_ids" eval="[(4, ref('sd_project_planning.group_sd_project_planning_admins'))]"/>
    </record>


</odoo>
